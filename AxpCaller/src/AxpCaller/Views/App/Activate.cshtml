@model AxpCaller.ViewModels.ActivateViewModel

@{
    ViewBag.Title = "Activate Page";
}

@section Scripts
{
    <script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>
}

<div>

    <form method="post" class="form-group">
        <div asp-validation-summary="ValidationSummary.ModelOnly"></div>
            <div>
                <label asp-for="CompanyID">CompanyID</label>
            </div>
            <textarea cols="100" rows="10" asp-for="CompanyID"></textarea>
            <span asp-for="CompanyID"></span>
            <div>
                <button type="submit" class="btn btn-default">Activate</button>
            </div>
</form>
</div>
